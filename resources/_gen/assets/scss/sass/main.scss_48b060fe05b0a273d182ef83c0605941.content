body{margin:0;padding:0;font-family:Geneva,Tahoma,Verdana,sans-serif;font-size:18px;color:"black";min-height:100vh;position:relative}body::after{content:'';display:block;height:50px}h1,h2,h3{color:#2f4f4f}h1{margin:0}h2{padding:1em 0 0;margin:0}div.top-title h2{padding:.5em 0}.post h2{margin:50px 0 0;padding:0;border-color:#000;border-style:groove none none;border-width:1px}a{text-decoration:none;color:blue;overflow-wrap:break-word;word-wrap:break-word}a:not(.btn):hover,a:not(.btn):focus,a:not(.btn):active{font-style:italic}a.menu-item{text-align:center;text-decoration:none;color:#000;padding:.25em .5em;font-size:1.2rem}a.menu-item:hover,a.menu-item:focus,a.menu-item:active{font-style:italic}.post-date{font-style:italic;font-size:15px;margin-top:10px;margin-bottom:25px}blockquote{border-left:.25em solid #d3d3d3;padding-left:1em;font-style:italic}header{padding:0;width:100%}header .menu{display:flex;align-items:center}header .menu .menu-item{padding:.5em 1em}header .menu-btn{display:none}header .menu-btn:checked~.menu-icon{z-index:10;padding:0 0 100vh 100vw;-webkit-tap-highlight-color:transparent}header .menu-btn:checked~.menu-icon .navicon-mid{background:0 0}header .menu-btn:checked~.menu-icon .navicon-bottom{transform:translateY(-11px)rotate(-45deg)}header .menu-btn:checked~.menu-icon .navicon-top{transform:translateY(11px)rotate(45deg)}header .menu-icon{cursor:pointer;flex-direction:column;padding:0}header .menu-icon .navicon-top,header .menu-icon .navicon-mid,header .menu-icon .navicon-bottom{background:#000;height:3px;width:40px;display:block;margin:4px 0;transition:all .2s ease-out}header .menu-icon .navicon-mid{transition:background .2s ease-out}.nav-container{max-width:100%;height:4rem;padding:.5em 0;display:flex;justify-content:space-between;align-items:center}.nav-left{display:flex;align-items:center}.hamburger{display:none}@media screen and (max-width:600px){h2{font-size:1.4em}.nav-container{align-items:flex-start;padding:.9em 0 0}.hamburger{display:flex}header .dropdown{display:flex;flex-direction:column;align-items:flex-end;padding:.3em 0;min-width:170px;width:40%}header .menu{width:200px;z-index:5;background:#fff;display:flex;flex-direction:column;align-items:center;visibility:hidden;opacity:0;padding:0;margin-top:.5em;border:.5px solid grey;transition:visibility .25s,opacity .25s ease-in-out}header .menu .menu-item{width:100%;padding:.7em 0}header .menu-btn:checked~.menu{position:absolute;top:60px;visibility:visible;opacity:1;z-index:15}}footer{position:absolute;bottom:0;width:100%;height:50px;background-color:#7affa6}footer .footer-content{height:50px;display:flex;align-items:center;justify-content:flex-end}footer .footer-content a{color:#000;padding:0 1em}.btn{display:inline-block;padding:8px 15px;color:#2f4f4f;background:#e3e3e3;text-decoration:none;border-radius:2px;margin-right:5px;margin-top:5px}.btn:hover,.btn:focus,.btn:active{color:#e3e3e3;background:#000}.btn-top{float:right;margin-top:23px}.btn-text:hover{font-style:italic}a:hover,a:focus,a:active{opacity:70%}a .social-media-thumbnail{max-height:27px;max-width:30px}div.top-title{background:#7affa6}figure{margin:20px 0;display:flex;justify-content:center;flex-direction:column;align-items:center;font-style:italic;font-size:15px}@media screen and (max-width:600px){figure img{max-width:100%!important}figure figcaption a{overflow-wrap:anywhere;word-wrap:anywhere}}ul{list-style:none;margin-left:1em;margin-bottom:2em;padding-left:0}ul li{margin:1em 1em 1em 0;padding-left:1em;text-indent:-1em}ul li:before{content:">";padding-right:5px}.container{max-width:700px;padding:0 1em;margin:0 auto}.nav{display:flex;justify-content:space-between;align-items:flex-end;flex-wrap:wrap}.nav-buttons{display:flex;justify-content:right;align-items:flex-end;flex-wrap:wrap}.no-bull-list{margin:10px 10px 30px 70px}.indent{margin-left:40px}span.italic{font-style:italic}span.underline{text-decoration:underline}span.bold{font-weight:700}.figcaption-text{margin:1em 1em 0;text-align:center}.toc_list{display:flex;flex-direction:column;align-items:center}.toc_list a{margin:0 .5em;padding:.3em 0;width:100%;text-align:center}#toc_container li,#toc_container ul,#toc_container ul li,#toc_container ul li:before{content:"";list-style:outside none none!important;margin:0 .5em;padding:.3em 0;width:100%;text-align:center}@media screen and (max-width:600px){.highlight{font-size:.7em}.highlight tbody tr td:first-child{display:none}}.MathJax{overflow-x:scroll;overflow-y:hidden;white-space:nowrap}code.has-jax{-webkit-font-smoothing:antialiased;background:inherit!important;border:none!important;font-size:100%}.centered{font-size:2rem;font-family:serif;text-align:center}.word-break{word-break:break-all}