body{margin:0;padding:0;font-family:Geneva,Tahoma,Verdana,sans-serif;font-size:18px;min-height:100vh;position:relative}h1,h2,h3{color:#2f4f4f}h1{margin:0}h2{padding:1em 0 0;margin:0}@media screen and (max-width:600px){h2{font-size:1.4em}}.post-date{font-style:italic;font-size:15px;margin-top:10px;margin-bottom:25px}@media screen and (max-width:600px){.listing{font-size:.7em}}a:hover,a:focus,a:active{opacity:70%}a:not(.img-link){text-decoration:none;color:blue;overflow-wrap:break-word;word-wrap:break-word}a:not(.img-link).menu-item{text-align:center;text-decoration:none;color:#000;font-size:1.2rem}a:not(.img-link).menu-item:hover,a:not(.img-link).menu-item:focus,a:not(.img-link).menu-item:active{font-style:italic}a.img-link{text-align:center;color:unset;text-decoration:unset}figure{margin:20px 0;display:flex;justify-content:center;flex-direction:column;align-items:center;font-style:italic;font-size:15px}@media screen and (max-width:600px){figure img{max-width:100%!important}figure figcaption a{overflow-wrap:anywhere;word-break:break-all}}.figcaption-text{margin:1em 1em 0;text-align:center}ul{list-style:none;margin-left:1em;margin-bottom:2em;padding-left:0}ul li{margin:1em 1em 1em 0;padding-left:1em;text-indent:-1em}ul li:before{content:"-";padding-right:5px}.container{max-width:700px;padding:0 1em;margin:0 auto}.toc_list{display:flex;flex-direction:column;align-items:center}.toc_list a{margin:0 .5em;padding:.3em 0;width:100%;text-align:center}#toc_container li,#toc_container ul,#toc_container ul li,#toc_container ul li:before{content:"";list-style:outside none none!important;margin:0 .5em;padding:.3em 0;width:100%;text-align:center}.highlight{overflow-y:auto}@media screen and (max-width:600px){.highlight{font-size:.7em}.highlight tbody tr td:first-child{display:none}}.MathJax{padding-top:.25em;padding-bottom:.25em;overflow-x:scroll;overflow-y:hidden;white-space:nowrap}code.has-jax{-webkit-font-smoothing:antialiased;background:inherit!important;border:none!important;font-size:100%}.centered{font-size:2rem;font-family:serif;text-align:center}.word-break{word-break:break-all}